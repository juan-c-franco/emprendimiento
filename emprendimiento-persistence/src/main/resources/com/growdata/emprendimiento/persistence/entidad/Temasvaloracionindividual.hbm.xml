<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.growdata.emprendimiento.persistence.entidad.Temasvaloracionindividual" table="TEMASVALORACIONINDIVIDUAL" schema="EMPRENDIMIENTOP" optimistic-lock="version">
        <id name="idtema" type="long">
            <column name="IDTEMA" precision="3" scale="0" />
            <generator class="assigned" />
        </id>
        <property name="nombretema" type="string">
            <column name="NOMBRETEMA" length="100" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>
